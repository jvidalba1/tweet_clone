<% if users.empty? %>
  <h4>Nothing yet</h4>
<% else %>
  <ul>
    <% users.each do |user| %>
      <li>
        <p>
          <%= user.full_name %> (<%= link_to("@#{user.username}", user_path(user.username)) %>)
          <%= render partial: "follow_button", locals: { user: user } %>
        </p>
      </li>
    <% end %>
  </ul>

  <%= will_paginate users %>
<% end %>
